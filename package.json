{
  "name": "bloomrpc",
  "productName": "BloomRPC",
  "version": "1.5.3",
  "description": "GRPC GUI client",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "build": {
    "productName": "BloomRPC",
    "appId": "io.github.utilitywarehouse.BloomRPC",
    "extraResources": [
      {
        "from": "static",
        "to": "static"
      }
    ],
    "extraFiles": [
      {
        "from": "resources/icon.ico",
        "to": "resources/icon.ico"
      }
    ],
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "about/about.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uw-labs/bloomrpc.git"
  },
  "author": {
    "name": "Fabrizio Fenoglio (UtilityWarehouse)"
  },
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/uw-labs/bloomrpc/issues"
  },
  "keywords": [
    "gRPC",
    "GUI",
    "bloomRPC",
    "BloomRPC",
    "gRPC GUI"
  ],
  "homepage": "https://github.com/uw-labs/bloomrpc",
  "jest": {
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "testMatch": [
      "**/__tests__/**/*.(js?(x)|ts?(x))",
      "**/?(*.)(spec|test).(js?(x)|ts?(x))"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFiles": [
      "./internals/scripts/CheckBuiltsExist.js"
    ]
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.9.2",
    "@grpc/proto-loader": "^0.7.9",
    "@types/jsdom": "^12.2.3",
    "@types/mousetrap": "^1.6.1",
    "@types/node": "^18.0.0",
    "@types/react": "^16.14.46",
    "@types/react-dom": "^16.9.19",
    "@types/react-test-renderer": "^16.9.6",
    "@types/uuid": "^9.0.3",
    "@types/validator": "^13.1.3",
    "@vitejs/plugin-react": "^4.0.4",
    "ace-builds": "^1.24.1",
    "antd": "3.26.7",
    "array-move": "^2.1.0",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "electron": "26.2.0",
    "electron-builder": "^24.6.4",
    "electron-store": "^8.1.0",
    "grpc-web": "^1.4.2",
    "identity-obj-proxy": "^3.0.0",
    "lodash.get": "^4.4.2",
    "mousetrap": "^1.6.3",
    "protobufjs": "^7.2.5",
    "re-resizable": "^4.11.0",
    "react": "^16.14.0",
    "react-ace": "^10.1.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.1",
    "react-sortable-hoc": "^1.9.1",
    "react-test-renderer": "^16.14.0",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "validator": "^13.5.2",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "devEngines": {
    "node": ">=8.12.x",
    "npm": ">=4.x",
    "yarn": ">=0.21.3"
  }
}
